<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gippy" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/_gippylib" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../gip/bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/_gippylib" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../gip/bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add directory="/usr/include/python2.7" />
			<Add directory="../gip" />
		</Compiler>
		<Linker>
			<Add library="gip" />
			<Add library="gdal" />
			<Add library="boost_system" />
			<Add library="boost_filesystem" />
			<Add library="boost_program_options" />
		</Linker>
		<ExtraCommands>
			<Add before="swig -c++ -python -w509 -I../gip gippylib.h" />
			<Add after='echo &quot;reg()&quot; &gt;&gt; gippylib.py' />
			<Add after='echo &quot;from gippy.GeoVector import *&quot; &gt;&gt; gippylib.py' />
		</ExtraCommands>
		<Unit filename="gippylib.h" />
		<Unit filename="gippylib_wrap.cxx" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
